html,body
  width: 100%
  height: 100%
  overflow-x: hidden
.clear-fix:after
  display: block
  width: 0
  height: 0
  content:''
  visibility: hidden
  clear: both
.hover-big:hover
  transform: scale(1.1,1.1)
  transition: transform 0.3s linear
.select-none
    -webkit-user-select:none
    -moz-user-select:none
    -ms-user-select:none
    user-select:none

